      <CodeComponent name="cpdm_pract_trial_py" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="#set default gray for all four response boxes&amp;#10;left_high_color = [0,0,0] &amp;#10;left_low_color = [0,0,0]&amp;#10;right_high_color = [0,0,0]&amp;#10;right_low_color = [0,0,0]" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="left_high_color = [0, 0, 0];&amp;#10;left_low_color = [0, 0, 0];&amp;#10;right_high_color = [0, 0, 0];&amp;#10;right_low_color = [0, 0, 0];&amp;#10;&amp;#10;rand_num = 0;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="my_loop = eval(practice_loop_name);&amp;#10;trial_acc = 0;&amp;#10;left_high_color = [0, 0, 0];&amp;#10;left_low_color = [0, 0, 0];&amp;#10;right_high_color = [0, 0, 0];&amp;#10;right_low_color = [0, 0, 0];&amp;#10;is_clockwise = false;&amp;#10;if ((cpdm_gabor_orient &gt; 0.0)) {&amp;#10;    is_clockwise = true;&amp;#10;} else {&amp;#10;    if ((cpdm_gabor_orient === 0.0)) {&amp;#10;        rand_num = Math.round(Math.random() + 1) ;&amp;#10;        if ((rand_num === 1)) {&amp;#10;            is_clockwise = true;&amp;#10;        } else {&amp;#10;            is_clockwise = false;&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="my_loop = eval(practice_loop_name)&amp;#10;trial_acc = 0 #initially defined as INCORRECT&amp;#10;trial_conf = 0 #initially defined as LOW CONF&amp;#10;&amp;#10;#set default gray for all four response boxes&amp;#10;left_high_color = [0,0,0] &amp;#10;left_low_color = [0,0,0]&amp;#10;right_high_color = [0,0,0]&amp;#10;right_low_color = [0,0,0]&amp;#10;&amp;#10;is_clockwise = False #preset correct tilt to left&amp;#10;if cpdm_gabor_orient &gt; 0.0: #if current trial gabor orientation is right&amp;#10;    is_clockwise = True #redefine correct tilt response to right&amp;#10;elif cpdm_gabor_orient == 0.0: #if current trial gabor orientation is vertical&amp;#10;    rand_num = random.randint(1,2) #randomly choose &quot;correct&quot; response&amp;#10;    if rand_num == 1: &amp;#10;        is_clockwise = True&amp;#10;    else:&amp;#10;        is_clockwise = False" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Py" valType="str" updates="None" name="Code Type"/>
        <Param val="##Each trial and resp&amp;#10;#checks which key Ss pressed and changes the color of the selected box&amp;#10;#depending on correctness of selection&amp;#10;&amp;#10;key = cpdm_pract_trial_resp.keys&amp;#10;if len(key) == 1: #Ss responds&amp;#10;    if key == &quot;1&quot;: #Ss selected LEFT/HIGH&amp;#10;        trial_conf = 1 #reassign to HIGH CONF&amp;#10;        if is_clockwise == False: #correct resp is LEFT&amp;#10;            left_high_color = &quot;green&quot; #change upper-left box to green&amp;#10;            trial_acc = 1 #reassign to CORRECT&amp;#10;        else: #correct resp is RIGHT&amp;#10;            left_high_color = &quot;red&quot;&amp;#10;    elif key == &quot;3&quot;: #Ss selected LEFT/LOW&amp;#10;        if is_clockwise == False: #correct resp is LEFT&amp;#10;            left_low_color = &quot;green&quot;&amp;#10;            trial_acc = 1&amp;#10;        else: #correct resp is RIGHT&amp;#10;            left_low_color = &quot;red&quot;&amp;#10;    elif key == &quot;2&quot;: #Ss selected RIGHT/HIGH&amp;#10;        trial_conf = 1&amp;#10;        if is_clockwise == True: #correct resp is RIGHT&amp;#10;            right_high_color = &quot;green&quot;&amp;#10;            trial_acc = 1&amp;#10;            trial_conf = 1&amp;#10;        else: #correct resp is LEFT&amp;#10;            right_high_color = &quot;red&quot;&amp;#10;    elif key == &quot;4&quot;: #Ss selected RIGHT/LOW&amp;#10;        if is_clockwise == True: #correct resp is RIGHT&amp;#10;            right_low_color = &quot;green&quot;&amp;#10;            trial_acc = 1&amp;#10;        else: #correct resp is LEFT&amp;#10;            right_low_color = &quot;red&quot;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="key = cpdm_pract_trial_resp.keys;&amp;#10;if (!(key === undefined) &amp;&amp; (key.length == 1)) {&amp;#10;    if ((key === &quot;q&quot;)) {&amp;#10;        trial_conf = 1;&amp;#10;        if ((is_clockwise === false)) {&amp;#10;            left_high_color = &quot;green&quot;;&amp;#10;            trial_acc = 1;&amp;#10;        } else {&amp;#10;            left_high_color = &quot;red&quot;;&amp;#10;        }&amp;#10;    } else {&amp;#10;        if ((key === &quot;a&quot;)) {&amp;#10;            if ((is_clockwise === false)) {&amp;#10;                left_low_color = &quot;green&quot;;&amp;#10;                trial_acc = 1;&amp;#10;            } else {&amp;#10;                left_low_color = &quot;red&quot;;&amp;#10;            }&amp;#10;        } else {&amp;#10;            if ((key === &quot;p&quot;)) {&amp;#10;                trial_conf = 1;&amp;#10;                if ((is_clockwise === true)) {&amp;#10;                    right_high_color = &quot;green&quot;;&amp;#10;                    trial_acc = 1;&amp;#10;                } else {&amp;#10;                    right_high_color = &quot;red&quot;;&amp;#10;                }&amp;#10;            } else {&amp;#10;                if ((key === &quot;l&quot;)) {&amp;#10;                    if ((is_clockwise === true)) {&amp;#10;                        right_low_color = &quot;green&quot;;&amp;#10;                        trial_acc = 1;&amp;#10;                    } else {&amp;#10;                        right_low_color = &quot;red&quot;;&amp;#10;                    }&amp;#10;                }&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="my_loop.addData(&quot;cpdm_acc&quot;, trial_acc);&amp;#10;my_loop.addData(&quot;cpdm_pract_conf&quot;, trial_conf);&amp;#10;my_loop.addData(&quot;cpdm_trial_type&quot;, &quot;practice&quot;);&amp;#10;&amp;#10;left_high_color = [0, 0, 0];&amp;#10;left_low_color = [0, 0, 0];&amp;#10;right_high_color = [0, 0, 0];&amp;#10;right_low_color = [0, 0, 0];&amp;#10;&amp;#10;cpdm_pract_trial_left_high_poly.setFillColor(new util.Color(left_high_color), false);&amp;#10;cpdm_pract_trial_left_low_poly.setFillColor(new util.Color(left_low_color), false);&amp;#10;cpdm_pract_trial_right_high_poly.setFillColor(new util.Color(right_high_color), false);&amp;#10;cpdm_pract_trial_right_low_poly.setFillColor(new util.Color(right_low_color), false);" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="my_loop.addData(&quot;cpdm_pract_acc&quot;, trial_acc) #record trial accuracy&amp;#10;my_loop.addData(&quot;cpdm_pract_conf&quot;, trial_conf) #record trial confidence&amp;#10;my_loop.addData(&quot;cpdm_trial_type&quot;, &quot;practice&quot;) #record current trial as &quot;practice&quot;&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
        <Param val="False" valType="bool" updates="None" name="disabled"/>
        <Param val="cpdm_pract_trial_py" valType="code" updates="None" name="name"/>
      </CodeComponent>
