      <CodeComponent name="idm_instr1_code" plugin="None">
        <Param val="###code written by Mandy Renfro (2022)###&amp;#10;&amp;#10;#allows all possible order of tasks (3)&amp;#10;import random&amp;#10;#QC and Bot check variable&amp;#10;exp_proceed = True&amp;#10;crdm_conf_resps = []&amp;#10;cdd_conf_resps = []&amp;#10;&amp;#10;#set empty array for Ss trials for possible payout across all tasks&amp;#10;cpdm_trials = []&amp;#10;crdm_trials = []&amp;#10;cdd_trials = []&amp;#10;&amp;#10;#CPDM&amp;#10;loop1 = False&amp;#10;#CRDM&amp;#10;loop2 = False &amp;#10;#CDD&amp;#10;loop3 = False &amp;#10;#CPDM&amp;#10;loop4 = False &amp;#10;#CRDM&amp;#10;loop5 = False &amp;#10;#CDD&amp;#10;loop6 = False &amp;#10;#CPDM&amp;#10;loop7 = False &amp;#10;&amp;#10;#random int determines order, loop# is used in should_run# nReps&amp;#10;#when converting to JS, use parseInt()&amp;#10;#order = int(random.random() * 6)&amp;#10;order = 1 #beta testing&amp;#10;#order 1&amp;#10;if order == 0: &amp;#10;    loop1 = True&amp;#10;    loop2 = True&amp;#10;    loop3 = True&amp;#10;#order 2&amp;#10;elif order == 1: &amp;#10;    loop1 = True&amp;#10;    loop3 = True&amp;#10;    loop5 = True&amp;#10;#order 3&amp;#10;elif order == 2: &amp;#10;    loop3 = True&amp;#10;    loop4 = True&amp;#10;    loop5 = True&amp;#10;#order 4&amp;#10;elif order == 3:&amp;#10;    loop2 = True&amp;#10;    loop3 = True&amp;#10;    loop4 = True&amp;#10;#order 5&amp;#10;elif order == 4: &amp;#10;    loop3 = True&amp;#10;    loop5 = True&amp;#10;    loop7 = True&amp;#10;#order 6&amp;#10;elif order == 5: &amp;#10;    loop2 = True&amp;#10;    loop4 = True&amp;#10;    loop6 = True" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="var cdd_conf_resps, cdd_trials, cpdm_trials, crdm_conf_resps, crdm_trials, exp_proceed, loop1, loop2, loop3, loop4, loop5, loop6, loop7, order;&amp;#10;exp_proceed = true;&amp;#10;crdm_conf_resps = [];&amp;#10;cdd_conf_resps = [];&amp;#10;cpdm_trials = [];&amp;#10;crdm_trials = [];&amp;#10;cdd_trials = [];&amp;#10;loop1 = false;&amp;#10;loop2 = false;&amp;#10;loop3 = false;&amp;#10;loop4 = false;&amp;#10;loop5 = false;&amp;#10;loop6 = false;&amp;#10;loop7 = false;&amp;#10;//order = 0;&amp;#10;order = Number.parseInt((Math.random() * 6));&amp;#10;if ((order === 0)) {&amp;#10;    loop1 = true;&amp;#10;    loop2 = true;&amp;#10;    loop3 = true;&amp;#10;} else {&amp;#10;    if ((order === 1)) {&amp;#10;        loop1 = true;&amp;#10;        loop3 = true;&amp;#10;        loop5 = true;&amp;#10;    } else {&amp;#10;        if ((order === 2)) {&amp;#10;            loop3 = true;&amp;#10;            loop4 = true;&amp;#10;            loop5 = true;&amp;#10;        } else {&amp;#10;            if ((order === 3)) {&amp;#10;                loop2 = true;&amp;#10;                loop3 = true;&amp;#10;                loop4 = true;&amp;#10;            } else {&amp;#10;                if ((order === 4)) {&amp;#10;                    loop3 = true;&amp;#10;                    loop5 = true;&amp;#10;                    loop7 = true;&amp;#10;                } else {&amp;#10;                    if ((order === 5)) {&amp;#10;                        loop2 = true;&amp;#10;                        loop4 = true;&amp;#10;                        loop6 = true;&amp;#10;                    }&amp;#10;                }&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
