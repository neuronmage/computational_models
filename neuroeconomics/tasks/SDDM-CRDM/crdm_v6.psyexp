        <Param val="idx = Number.parseInt((random.random() * rewards.length));&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="#indexing examples&amp;#10;#certain chosen - (&quot;sure&quot;, crdm_sure_amt, crdm_img, crdm_lott_top, crdm_lott_bot, crdm_win_side, crdm_domain)&amp;#10;#lotto chosen - (&quot;lott&quot;, &quot;?&quot;, crdm_img, crdm_lott_top, crdm_lott_bot, crdm_win_side, crdm_domain))&amp;#10;&amp;#10;earnings_text = &quot;&quot;&amp;#10;idx = int(random.random() * len(earnings)) #random selection of bonus trial&amp;#10;print(earnings[idx])&amp;#10;choice, money, trial_img, top, bottom, winning_side, domain = earnings[idx]&amp;#10;my_loop.addData(&quot;bonus_choice&quot;, choice) #record bonus trial choice&amp;#10;&amp;#10;# Describe trial image&amp;#10;trial_img_desc = &quot;&quot;&amp;#10;if trial_img == &quot;ambig_24.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was ambiguous, with 24% hidden (divided into two sections of 38%).&quot;&amp;#10;elif trial_img == &quot;ambig_50.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was ambiguous, with 50% hidden (divided into two sections of 25%).&quot;&amp;#10;elif trial_img == &quot;ambig_74.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was ambiguous, with 74% hidden (divided into two sections of 13%).&quot;&amp;#10;elif trial_img == &quot;risk_blue_13.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 87% probability and blue having 13% probability.&quot;&amp;#10;elif trial_img == &quot;risk_blue_25.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 75% probability and blue having 25% probability.&quot;&amp;#10;elif trial_img == &quot;risk_blue_38.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 62% probability and blue having 38% probability.&quot;&amp;#10;elif trial_img == &quot;risk_blue_50.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 50% probability and blue having 50% probability.&quot;&amp;#10;elif trial_img == &quot;risk_blue_75.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 25% probability and blue having 75% probability.&quot;&amp;#10;elif trial_img == &quot;risk_red_13.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 13% probability and blue having 87% probability.&quot;&amp;#10;elif trial_img == &quot;risk_red_25.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 25% probability and blue having 75% probability.&quot;&amp;#10;elif trial_img == &quot;risk_red_38.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 38% probability and blue having 62% probability.&quot;&amp;#10;elif trial_img == &quot;risk_red_50.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 50% probability and blue having 50% probability.&quot;&amp;#10;elif trial_img == &quot;risk_red_75.bmp&quot;:&amp;#10;    trial_img_desc = &quot;The lottery chosen was risky, with red having 75% probability and blue having 25% probability.&quot;&amp;#10;&amp;#10;#text for bonus payment screen&amp;#10;if choice == &quot;sure&quot;: ##Ss chose certain $5&amp;#10;    earnings_text = &quot;A certain $&quot; + str(money) # &quot;In the randomly selected trial, you chose: A CERTAIN $&quot; + str(money)&amp;#10;else: ##Ss chose lottery&amp;#10;    earnings_text = &quot;Play the lottery! Draw a chip from the bag with the red amount being $&quot; + str(top) + &quot; and the blue amount being $&quot; + str(bottom) + &quot;.\n&quot; + trial_img_desc # &quot;In the randomly selected trial, you chose: \n \n PLAY THE LOTTERY! \n \n You will now draw a chip from the lottery bag with these possible values. \n \n Good luck!&quot;&amp;#10;    &amp;#10;# Print the bonus to the log, just in case&amp;#10;print(&quot;Bonus:&quot;)&amp;#10;print(earnings_text)&amp;#10;print(&quot;Top: &quot;, top)&amp;#10;print(&quot;Bottom: &quot;, bottom)&amp;#10;print(&quot;Choice: &quot;, choice)&amp;#10;&amp;#10;# Create a file that has all of the bonuses&amp;#10;# Put into a higher directory containing only bonuses&amp;#10;filename = u'../../../datasets/SDDM/bonus'&amp;#10;&amp;#10;if not os.path.exists(filename):&amp;#10;    os.makedirs(filename)&amp;#10;    &amp;#10;os.chdir(filename)&amp;#10;&amp;#10;# Remove the participant's task from participant's name -- that is, only get the last four characters&amp;#10;participant_name = expInfo['participant'][-4:]&amp;#10;&amp;#10;# Create the bonus &quot;phrase&quot; that will go into the file&amp;#10;bonus_filename = &quot;bonus_all_&quot; + participant_name + expInfo['session'] + &quot;.txt&quot;&amp;#10;&amp;#10;# If the file already exists, append to the end. If not, create new file&amp;#10;f = open(bonus_filename, &quot;a&quot;) &amp;#10;&amp;#10;# Write the bonus phrase &amp;#10;bonus_phrase = &quot;CRDM bonus is: &quot; + earnings_text + &quot;\n&quot;&amp;#10;f.write(bonus_phrase)&amp;#10;f.close()" valType="extendedCode" updates="constant" name="Begin Routine"/>
