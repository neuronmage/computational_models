        <Param val="#indexing example&amp;#10;#certain chosen - (&quot;sure&quot;, 5, img, lott_top, lott_bot, &quot;-1&quot;, win_side, domain)&amp;#10;#lotto chosen, win top - (&quot;lott&quot;, lott_top, img, lott_top, lott_bot, lott_outcome, win_side, domain)&amp;#10;#lotto chosen, win bottom - (&quot;lott&quot;, lott_bot, img, lott_top, lott_bot, lott_outcome, win_side, domain)&amp;#10;#lotto chosen, lose - (&quot;lott&quot;, 0, img, lott_top, lott_bot, lott_outcome, win_side, domain)&amp;#10;&amp;#10;&amp;#10;#random selection of bonus trial&amp;#10;idx = int(random.random() * len(earnings)) &amp;#10;print(earnings[idx])&amp;#10;choice, money, trial_img, top, bottom, outcome, winning_side, domain = earnings[idx]&amp;#10;sure_amt = 5&amp;#10;my_loop.addData(&quot;bonus_choice&quot;, choice)&amp;#10;my_loop.addData(&quot;bonus_amount&quot;, money)&amp;#10;&amp;#10;red_win = False&amp;#10;#red wins in non-ambiguous trial&amp;#10;if &quot;red&quot; in trial_img: &amp;#10;    red_win = True&amp;#10;#blue wins or ambiguous red trial&amp;#10;else: &amp;#10;    #ambiguous trial&amp;#10;    if &quot;ambig&quot; in trial_img: &amp;#10;        #red wins&amp;#10;        if winning_side == &quot;top&quot;: &amp;#10;            red_win = True&amp;#10;&amp;#10;#text for bonus payment screen&amp;#10;#Ss choice certain $5&amp;#10;if choice == &quot;sure&quot;: &amp;#10;    choice_text = &quot;*CERTAIN*&quot;&amp;#10;    outcome_color = &quot;goldenrod&quot;&amp;#10;    chip_color = [0,0,0]&amp;#10;    chip_text = &quot;&quot;&amp;#10;    choice_outcome = &quot;$&quot; + str(money)&amp;#10;    money_outcome = &quot;&quot;&amp;#10;&amp;#10;else: #Ss chose lottery&amp;#10;    choice_text = &quot;*PLAY THE LOTTERY*&quot;&amp;#10;    #Ss won&amp;#10;    if outcome == 1: &amp;#10;        if red_win == True:&amp;#10;            outcome_color = &quot;salmon&quot;&amp;#10;            chip_color = &quot;salmon&quot;&amp;#10;            choice_outcome = &quot;A red chip&quot;&amp;#10;   