      <CodeComponent name="rdm_trial_code" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="#used in variable ITI&amp;#10;delta_time = 0 &amp;#10;#position of certain amount option&amp;#10;sure_pos = [] &amp;#10;sure_resp = []&amp;#10;crdm_msg = &quot;&quot;&amp;#10;&amp;#10;#stimuli positions and respective responses&amp;#10;#left/right screen locations&amp;#10;pos = [[-0.3, 0], [0.3, 0]] &amp;#10;#1 = left, 2 = right&amp;#10;resp = [&quot;1&quot;, &quot;2&quot;] " valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="delta_time = 0;&amp;#10;sure_pos = [];&amp;#10;sure_resp = [];&amp;#10;crdm_msg = &quot;&quot;;&amp;#10;pos = [[(- 0.3), 0], [0.3, 0]];&amp;#10;resp = [&quot;1&quot;, &quot;2&quot;];&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="my_loop = eval(loop_name);&amp;#10;stop_timer = null;&amp;#10;stopped_time = 0;&amp;#10;lott_outcome = 0;&amp;#10;idx = random.randint(0, 1);&amp;#10;sure_pos = pos[idx];&amp;#10;sure_resp = resp[idx];&amp;#10;sure_amt = 5;&amp;#10;if ((idx === 0)) {&amp;#10;    lott_pos = pos[1];&amp;#10;} else {&amp;#10;    lott_pos = pos[0];&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="#variable for saving data&amp;#10;my_loop = eval(loop_name) &amp;#10;stop_timer = None &amp;#10;stopped_time = 0&amp;#10;lott_outcome = 0&amp;#10;&amp;#10;#random index for certain outcome position and response&amp;#10;idx = random.randint(0,1) &amp;#10;sure_pos = pos[idx] &amp;#10;sure_resp = resp[idx]&amp;#10;sure_amt = 5&amp;#10;&amp;#10;#certain outcome screen position is left/right, lottery is opposite&amp;#10;if idx == 0: &amp;#10;    lott_pos = pos[1]&amp;#10;else:&amp;#10;    lott_pos = pos[0]" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Both" valType="str" updates="None" name="Code Type"/>
        <Param val="#Ss responded&amp;#10;if len(rdm_trial_resp.keys) == 1: &amp;#10;    if stop_timer == None: &amp;#10;        stop_timer = core.Clock() &amp;#10;    else:&amp;#10;        if stop_timer.getTime() &gt;= 0.5:&amp;#10;            continueRoutine = False" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="if (!(rdm_trial_resp.keys === undefined) &amp;&amp; (rdm_trial_resp.keys.length === 1)) {&amp;#10;    if ((stop_timer === null)) {&amp;#10;        stop_timer = new core.Clock();&amp;#10;    } else {&amp;#10;        if ((stop_timer.getTime() &gt;= 0.5)) {&amp;#10;            continueRoutine = false;&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="key = rdm_trial_resp.keys; &amp;#10;sure_key = rdm_trial_resp.corr; &amp;#10;&amp;#10;if ((((rdm_trial_resp.rt) === undefined) || (rdm_trial_resp.rt &gt;= (2.2 - 0.5)))) {&amp;#10;    delta_time = 0;&amp;#10;} else {&amp;#10;    delta_time = Math.max(0, (2.2 - (rdm_trial_resp.rt + stopped_time)));&amp;#10;}&amp;#10;if (!(rdm_trial_resp.keys === undefined) &amp;&amp; (rdm_trial_resp.keys.length === 1)) {&amp;#10;    if ((sure_key === 1)) {&amp;#10;        earnings.push([&quot;sure&quot;, 5, img, lott_top, lott_bot, (-1), win_side, domain]);&amp;#10;        my_loop.addData(&quot;crdm_choice&quot;, &quot;sure&quot;);&amp;#10;    } else {&amp;#10;        lott_outcome = random.choices([0, 1], [100 - Number.parseInt(lott_p), Number.parseInt(lott_p)]);&amp;#10;        my_loop.addData(&quot;crdm_choice&quot;, &quot;lott&quot;);&amp;#10;        my_loop.addData(&quot;lott_outcome&quot;, lott_outcome);&amp;#10;        if ((lott_outcome === 1)) {&amp;#10;            if ((Number.parseInt(lott_top) !== 0)) {&amp;#10;                earnings.push([&quot;lott&quot;, lott_top, img, lott_top, lott_bot, lott_outcome, win_side, domain]);&amp;#10;            } else {&amp;#10;                earnings.push([&quot;lott&quot;, lott_bot, img, lott_top, lott_bot, lott_outcome, win_side, domain]);&amp;#10;            }&amp;#10;        } else {&amp;#10;            earnings.push([&quot;lott&quot;, 0, img, lott_top, lott_bot, lott_outcome, win_side, domain]);&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="#variable for feedback routine&amp;#10;key = rdm_trial_resp.keys &amp;#10;#1 if certain $ selected&amp;#10;sure_key = rdm_trial_resp.corr &amp;#10;&amp;#10;#delta time is leftover time excluding rt&amp;#10;if type(rdm_trial_resp.rt) is list or rdm_trial_resp.rt &gt;= 2 - 0.5: &amp;#10;    delta_time = 0&amp;#10;else:&amp;#10;    delta_time = max(0, 2 - (rdm_trial_resp.rt + stopped_time))&amp;#10;&amp;#10;#code to conduct chip draw using bag probabilities and store/record outcome&amp;#10;#response received&amp;#10;if key == sure_resp: &amp;#10;    #certain option&amp;#10;    if sure_key == 1: &amp;#10;        #-1 is placeholder for lott_outcome&amp;#10;        earnings.append((&quot;sure&quot;, 5, img, lott_top, lott_bot, -1, win_side, domain))&amp;#10;        my_loop.addData(&quot;crdm_choice&quot;, &quot;sure&quot;)&amp;#10;    #lottery option&amp;#10;    else: &amp;#10;        #probability from bag parameters&amp;#10;        lott_outcome = random.choices([0,1], weights=[100-int(lott_p), int(lott_p)])[0] &amp;#10;        #record lottery outcome&amp;#10;        my_loop.addData(&quot;crdm_choice&quot;, &quot;lott&quot;)&amp;#10;        my_loop.addData(&quot;lott_outcome&quot;, lott_outcome) &amp;#10;        #win&amp;#10;        if lott_outcome == 1: &amp;#10;            #check which side was worth money&amp;#10;            if int(lott_top) != 0: &amp;#10;                #top side is winner&amp;#10;                earnings.append((&quot;lott&quot;, lott_top, img, lott_top, lott_bot, lott_outcome, win_side, domain)) &amp;#10;            else: &amp;#10;                #bottom side is winner&amp;#10;                earnings.append((&quot;lott&quot;, lott_bot, img, lott_top, lott_bot, lott_outcome, win_side, domain)) &amp;#10;        #lose&amp;#10;        else: &amp;#10;            #losing lottery&amp;#10;            earnings.append((&quot;lott&quot;, 0, img, lott_top, lott_bot, lott_outcome, win_side, domain)) " valType="extendedCode" updates="constant" name="End Routine"/>
        <Param val="False" valType="bool" updates="None" name="disabled"/>
