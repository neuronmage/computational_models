        <Param val="var cdd_conf_resps, cdd_trials, cpdm_trials, crdm_conf_resps, crdm_trials, exp_proceed, loop1, loop2, loop3, loop4, loop5, loop6, loop7, order;&amp;#10;exp_proceed = true;&amp;#10;crdm_conf_resps = [];&amp;#10;cdd_conf_resps = [];&amp;#10;cpdm_trials = [];&amp;#10;crdm_trials = [];&amp;#10;cdd_trials = [];&amp;#10;loop1 = false;&amp;#10;loop2 = false;&amp;#10;loop3 = false;&amp;#10;loop4 = false;&amp;#10;loop5 = false;&amp;#10;loop6 = false;&amp;#10;loop7 = false;&amp;#10;order = 0;&amp;#10;//order = Number.parseInt((Math.random() * 6));&amp;#10;if ((order === 0)) {&amp;#10;    loop1 = true;&amp;#10;    loop2 = true;&amp;#10;    loop3 = true;&amp;#10;} else {&amp;#10;    if ((order === 1)) {&amp;#10;        loop1 = true;&amp;#10;        loop3 = true;&amp;#10;        loop5 = true;&amp;#10;    } else {&amp;#10;        if ((order === 2)) {&amp;#10;            loop3 = true;&amp;#10;            loop4 = true;&amp;#10;            loop5 = true;&amp;#10;        } else {&amp;#10;            if ((order === 3)) {&amp;#10;                loop2 = true;&amp;#10;                loop3 = true;&amp;#10;                loop4 = true;&amp;#10;            } else {&amp;#10;                if ((order === 4)) {&amp;#10;                    loop3 = true;&amp;#10;                    loop5 = true;&amp;#10;                    loop7 = true;&amp;#10;                } else {&amp;#10;                    if ((order === 5)) {&amp;#10;                        loop2 = true;&amp;#10;                        loop4 = true;&amp;#10;                        loop6 = true;&amp;#10;                    }&amp;#10;                }&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
