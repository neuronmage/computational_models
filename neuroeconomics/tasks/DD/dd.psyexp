        <Param val="my_loop = eval(loop_name);&amp;#10;stop_timer = null;&amp;#10;stopped_time = 0;&amp;#10;idx = random.randint(0, 1);&amp;#10;immed_pos = pos[idx];&amp;#10;immed_resp = resp[idx];&amp;#10;if ((idx === 0)) {&amp;#10;    delay_pos = pos[1];&amp;#10;} else {&amp;#10;    delay_pos = pos[0];&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="my_loop = eval(loop_name) &amp;#10;stop_timer = None&amp;#10;stopped_time = 0&amp;#10;&amp;#10;#random integer to determine side of immediate option&amp;#10;idx = random.randint(0,1) &amp;#10;&amp;#10;#defines position of immed option&amp;#10;immed_pos = pos[idx] &amp;#10;#defines response button for immed option&amp;#10;immed_resp = resp[idx] &amp;#10;&amp;#10;#immed position is left, delay is  right&amp;#10;if idx == 0: &amp;#10;    delay_pos = pos[1]&amp;#10;else: &amp;#10;    delay_pos = pos[0]" valType="extendedCode" updates="constant" name="Begin Routine"/>
