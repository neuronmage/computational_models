        <Param val="exp = [];&amp;#10;choice = [];&amp;#10;choice_amt = [];&amp;#10;choice_wait = [];&amp;#10;other_amt = [];&amp;#10;other_wait = [];&amp;#10;choice_text = &quot;&quot;;&amp;#10;other_text = &quot;&quot;;&amp;#10;idx = Number.parseInt((random.random() * cdd_trials.length));&amp;#10;if ((cdd_trials[idx][1] === &quot;immed&quot;)) {&amp;#10;    [exp, choice, choice_amt, choice_wait, other_amt, other_wait] = cdd_trials[idx];&amp;#10;    choice_text = ((&quot;$&quot; + choice_amt.toString()) + &quot; TODAY&quot;);&amp;#10;    other_text = ((((&quot;$&quot; + other_amt.toString()) + &quot; in &quot;) + other_wait.toString()) + &quot; DAYS&quot;);&amp;#10;    if ((random_exp === &quot;cdd&quot;)) {&amp;#10;        my_loop.addData(&quot;cdd_bonus_choice&quot;, choice);&amp;#10;        my_loop.addData(&quot;cdd_bonus_amt&quot;, choice_amt);&amp;#10;        my_loop.addData(&quot;cdd_bonus_delay&quot;, choice_wait);&amp;#10;    }&amp;#10;} else {&amp;#10;    [exp, choice, other_amt, other_wait, choice_amt, choice_wait] = cdd_trials[idx];&amp;#10;    choice_text = ((((&quot;$&quot; + choice_amt.toString()) + &quot; in &quot;) + choice_wait.toString()) + &quot; DAYS&quot;);&amp;#10;    other_text = ((&quot;$&quot; + other_amt.toString()) + &quot; TODAY&quot;);&amp;#10;    if ((random_exp === &quot;cdd&quot;)) {&amp;#10;        my_loop.addData(&quot;cdd_bonus_choice&quot;, choice);&amp;#10;        my_loop.addData(&quot;cdd_bonus_amt&quot;, choice_amt);&amp;#10;        my_loop.addData(&quot;cdd_bonus_delay&quot;, choice_wait);&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="exp = []&amp;#10;choice = []&amp;#10;choice_amt = []&amp;#10;choice_wait = []&amp;#10;other_amt = []&amp;#10;other_wait = []&amp;#10;choice_text = &quot;&quot;&amp;#10;other_text = &quot;&quot;&amp;#10;&amp;#10;idx = int(random.random() * len(earnings)) &amp;#10;#(&quot;CDD&quot;, &quot;immed&quot;, immed_amt, immed_wait, delay_amt, delay_wait) &amp;#10;if earnings[idx][1] == &quot;immed&quot;: #Ss chose immediate amount on selected bonus trial&amp;#10;    exp, choice, choice_amt, choice_wait, other_amt, other_wait = earnings[idx]&amp;#10;    choice_text = &quot;$&quot; + str(choice_amt) + &quot; TODAY&quot;&amp;#10;    other_text = &quot;$&quot; + str(other_amt) + &quot; in &quot; + str(other_wait) + &quot; DAYS&quot;&amp;#10;    my_loop.addData(&quot;cdd_bonus_choice&quot;, choice)&amp;#10;    my_loop.addData(&quot;cdd_bonus_amt&quot;, choice_amt)&amp;#10;    my_loop.addData(&quot;cdd_bonus_delay&quot;, choice_wait)&amp;#10;else: #Ss chose delay amount on selected bonus trial&amp;#10;    exp, choice, other_amt, other_wait, choice_amt, choice_wait = earnings[idx]&amp;#10;    choice_text = &quot;$&quot; + str(choice_amt) + &quot; in &quot; + str(choice_wait) + &quot; DAYS&quot;&amp;#10;    other_text = &quot;$&quot; + str(other_amt) + &quot; TODAY&quot;&amp;#10;    my_loop.addData(&quot;cdd_bonus_choice&quot;, choice)&amp;#10;    my_loop.addData(&quot;cdd_bonus_amt&quot;, choice_amt)&amp;#10;    my_loop.addData(&quot;cdd_bonus_delay&quot;, choice_wait)&amp;#10;    &amp;#10;# Print the bonus to the log, just in case&amp;#10;print(&quot;Bonus:&quot;)&amp;#10;print(choice_text) # What the participant gets&amp;#10;print(&quot;rather than&quot;)&amp;#10;print(other_text) # What the participant did not get&amp;#10;&amp;#10;# Create a file that has all of the bonuses&amp;#10;# Put into a higher directory containing only bonuses&amp;#10;filename = u'../../../datasets/SDDM/bonus'&amp;#10;&amp;#10;if not os.path.exists(filename):&amp;#10;    os.makedirs(filename)&amp;#10;    &amp;#10;os.chdir(filename)&amp;#10;&amp;#10;# Remove the participant's task from participant's name -- that is, only get the last four characters&amp;#10;participant_name = expInfo['participant'][-4:]&amp;#10;&amp;#10;# Create the bonus &quot;phrase&quot; that will go into the file&amp;#10;bonus_filename = &quot;bonus_all_&quot; + participant_name + expInfo['session'] + &quot;.txt&quot;&amp;#10;&amp;#10;# If the file already exists, append to the end. If not, create new file&amp;#10;f = open(bonus_filename, &quot;a&quot;) &amp;#10;&amp;#10;# Write the bonus phrase &amp;#10;bonus_phrase = &quot;CDD bonus is: &quot; + choice_text + &quot; rather than &quot; + other_text + &quot;\n&quot;&amp;#10;f.write(bonus_phrase)&amp;#10;f.close()&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
